<resources>

    <!-- Light / Regular style fonts -->
    <java-symbol type="string" name="config_lightFontFamily" />
    <java-symbol type="string" name="config_regularFontFamily" />

    <!-- Whether to cleanup fingerprints upon connection to the daemon and when
         user switches -->
    <java-symbol type="bool" name="config_cleanupUnusedFingerprints" />

    <!-- Whether to allow process with media UID to access CameraServiceProxy -->
    <java-symbol type="bool" name="config_allowMediaUidForCameraServiceProxy" />

    <!-- Whether to cancel fingerprint operation if not idle -->
    <java-symbol type="bool" name="config_fpCancelIfNotIdle" />

    <!-- Whether device supports an alt. ambient display -->
    <java-symbol type="bool" name="config_alt_ambient_display"/>

    <!-- Whether Pixel props is enabled -->
    <java-symbol type="bool" name="config_enablePixelProps" />

    <!-- Location indicator -->
    <java-symbol type="drawable" name="stat_sys_location" />

    <!-- SystemUI Restart -->
    <java-symbol type="string" name="systemui_restart_title" />
    <java-symbol type="string" name="systemui_restart_message" />

    <!-- Cycle through ringer modes -->
    <java-symbol type="string" name="volume_dialog_ringer_guidance_normal" />

    <!-- Device keyhandlers -->
    <java-symbol type="array" name="config_deviceKeyHandlerLibs" />
    <java-symbol type="array" name="config_deviceKeyHandlerClasses" />

    <!-- Prop/KeyboxImitationHooks -->
    <java-symbol type="array" name="config_certifiedKeybox" />

    <!-- Advanced reboot -->
    <java-symbol type="string" name="reboot_to_recovery_title" />
    <java-symbol type="string" name="reboot_to_recovery_message" />
    <java-symbol type="string" name="reboot_to_bootloader_title" />
    <java-symbol type="string" name="reboot_to_bootloader_message" />
    <java-symbol type="string" name="reboot_title" />
    <java-symbol type="string" name="reboot_message" />

    <!-- Global Actions pref list -->
    <java-symbol type="array" name="custom_config_globalActionsList" />

    <!-- Camera -->
    <java-symbol type="array" name="config_cameraAuxPackageAllowList" />
    <java-symbol type="array" name="config_cameraAuxPackageExcludeList" />

    <!-- Should we listen for fingerprints when the screen is off?  Devices
         with a rear-mounted sensor want this, but certain devices have
         the sensor embedded in the power key and listening all the time
         causes a poor experience. -->
    <java-symbol type="bool" name="config_fingerprintWakeAndUnlock" />

    <!-- Whether device has physical tri state switch -->
    <java-symbol type="bool" name="config_hasAlertSlider" />

    <!-- The location of the devices physical tri state switch -->
    <java-symbol type="integer" name="config_alertSliderLocation" />

    <!-- Whether key handler sends intent when changing slider position -->
    <java-symbol type="string" name="config_alertSliderIntent" />

    <!-- Landscape Battery -->
    <java-symbol type="string" name="config_batterymeterRLandPerimeterPath" />
    <java-symbol type="string" name="config_batterymeterRLandFillMask" />
    <java-symbol type="string" name="config_batterymeterRLandBoltPath" />
    <java-symbol type="string" name="config_batterymeterRLandPowersavePath" />
    <java-symbol type="string" name="config_batterymeterRLandErrorPerimeterPath" />
    <java-symbol type="string" name="config_batterymeterLandPerimeterPath" />
    <java-symbol type="string" name="config_batterymeterLandFillMask" />
    <java-symbol type="string" name="config_batterymeterLandBoltPath" />
    <java-symbol type="string" name="config_batterymeterLandPowersavePath" />
    <java-symbol type="string" name="config_batterymeterLandErrorPerimeterPath" />

</resources>
